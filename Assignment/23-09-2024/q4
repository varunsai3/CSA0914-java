class Vehicle {
String fuelType;
int speed;

Vehicle(String fuelType, int speed) {
    this.fuelType = fuelType;
    this.speed = speed;
}

void displayVehicleInfo() {
    System.out.println("Fuel Type: " + fuelType);
    System.out.println("Speed: " + speed + " km/h");
}
}

class Car extends Vehicle {
String brand;

Car(String fuelType, int speed, String brand) {
    super(fuelType, speed);
    this.brand = brand;
}

void displayCarInfo() {
    displayVehicleInfo();
    System.out.println("Brand: " + brand);
}
}

class ElectricCar extends Car {
int batteryCapacity;

ElectricCar(String fuelType, int speed, String brand, int batteryCapacity) {
    super(fuelType, speed, brand);
    this.batteryCapacity = batteryCapacity;
}

void displayElectricCarInfo() {
    displayCarInfo();
    System.out.println("Battery Capacity: " + batteryCapacity + " kWh");
}
}

public class MultilevelInheritanceExample {
public static void main(String[] args) {
ElectricCar electricCar = new ElectricCar("Electric", 150, "Tesla", 75);
electricCar.displayElectricCarInfo();
}
}
